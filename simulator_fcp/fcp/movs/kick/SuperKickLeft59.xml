        <move id="&lleg3;" angle="40"/>
        <move id="&lleg4;" angle="-80"/>
        <move id="&lleg5;" angle="40"/>
    </slot>

    <slot name="leftLegPushBack" delta="0.500">
        <move id="&rleg2;" angle="12"/>
        <move id="&rleg6;" angle="-12"/>

        <move id="&rleg3;" angle="30"/>
