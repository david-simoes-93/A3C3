        <move id="&lleg3;" angle="-40"/>
        <move id="&lleg4;" angle="-70"/>
        <move id="&lleg5;" angle="75"/>
    </slot>

    <slot name="kickLeft1" delta="0.020">
        <move id="&larm1;" angle="0"/>
        <move id="&rarm1;" angle="0"/>

        <move id="&rleg3;" angle="-20"/>
        <move id="&rleg4;" angle="-20"/>

        <move id="&lleg3;" angle="20"/>
        <move id="&lleg4;" angle="-120"/>
        <move id="&lleg5;" angle="58"/>
