    <!-- getting up -->
    <slot delta=".100"/>

    <slot delta=".400">
        <move id="&head1;" angle="0"/>
        <move id="&head2;" angle="0"/>
        <move id="&lleg1;" angle="0"/>
        <move id="&rleg1;" angle="0"/>
        <move id="&lleg2;" angle="0"/>
        <move id="&rleg2;" angle="0"/>
        <move id="&lleg3;" angle="0"/>
        <move id="&rleg3;" angle="0"/>
        <move id="&lleg4;" angle="0"/>
        <move id="&rleg4;" angle="0"/>
        <move id="&lleg5;" angle="0"/>
        <move id="&rleg5;" angle="0"/>
        <move id="&lleg6;" angle="0"/>
        <move id="&rleg6;" angle="0"/>
        <move id="&larm1;" angle="-90"/>
        <move id="&rarm1;" angle="-90"/>
        <move id="&larm2;" angle="0"/>
        <move id="&rarm2;" angle="0"/>
        <move id="&larm3;" angle="0"/>
        <move id="&rarm3;" angle="0"/>
        <move id="&larm4;" angle="0"/>
        <move id="&rarm4;" angle="0"/>
    </slot>

    <slot delta=".200">
        <move id="&larm1;" angle="0"/>
        <move id="&rarm1;" angle="0"/>
        <move id="&larm2;" angle="0"/>
        <move id="&rarm2;" angle="0"/>
    </slot>

    <slot delta=".200"/>

    <slot delta=".200">
        <move id="&lleg1;" angle="-40"/>
        <move id="&rleg1;" angle="-40"/>
        <move id="&lleg2;" angle="35"/>
        <move id="&rleg2;" angle="-35"/>
        <move id="&lleg6;" angle="-40"/>
        <move id="&rleg6;" angle="40"/>
        <move id="&lleg4;" angle="-100"/>
        <move id="&rleg4;" angle="-100"/>
        <move id="&lleg5;" angle="60"/>
        <move id="&rleg5;" angle="60"/>
    </slot>

    <slot delta=".280">
        <move id="&lleg3;" angle="100"/>
        <move id="&rleg3;" angle="100"/>
    </slot>

    <slot delta=".300">
        <move id="&lleg2;" angle="0" kp="0.6"/>
        <move id="&rleg2;" angle="0" kp="0.6"/>
        <move id="&lleg6;" angle="-20" kp="0.6"/>
        <move id="&rleg6;" angle="20" kp="0.6"/>
        <move id="&lleg4;" angle="-130" kp="0.6"/>
        <move id="&rleg4;" angle="-130" kp="0.6"/>
    </slot>

    <slot delta=".300">
        <move id="&lleg1;" angle="0"/>
        <move id="&rleg1;" angle="0"/>
        <move id="&lleg6;" angle="0"/>
        <move id="&rleg6;" angle="0"/>
    </slot>

    <slot delta="1.500">
        <move id="&larm1;" angle="-90"/>
        <move id="&rarm1;" angle="-90"/>
        <move id="&lleg3;" angle="0"/>
        <move id="&rleg3;" angle="0"/>
        <move id="&lleg4;" angle="0"/>
        <move id="&rleg4;" angle="0"/>
        <move id="&lleg5;" angle="0"/>
        <move id="&rleg5;" angle="0"/>
    </slot>

    <slot delta=".100"/>
</behavior>

