<?xml version="1.0" encoding="ISO-8859-1"?>

<!DOCTYPE joints [
        <!ENTITY head1  "0" >
        <!ENTITY head2  "1" >
        <!ENTITY lleg1  "2" >
        <!ENTITY rleg1  "3" >
        <!ENTITY lleg2  "4" >
        <!ENTITY rleg2  "5" >
        <!ENTITY lleg3  "6" >
        <!ENTITY rleg3  "7" >
        <!ENTITY lleg4  "8" >
        <!ENTITY rleg4  "9" >
        <!ENTITY lleg5  "10"    >
        <!ENTITY rleg5  "11"    >
        <!ENTITY lleg6  "12"    >
        <!ENTITY rleg6  "13"    >
        <!ENTITY larm1  "14"    >
        <!ENTITY rarm1  "15"    >
        <!ENTITY larm2  "16"    >
        <!ENTITY rarm2  "17"    >
        <!ENTITY larm3  "18"    >
        <!ENTITY rarm3  "19"    >
        <!ENTITY larm4  "20"    >
        <!ENTITY rarm4  "21"    >
        <!ENTITY amp1 "12"> <!-- ankle + thigh amp 24 -->
        <!ENTITY amp2 "24"> <!-- knee amp 48 -->
        <!ENTITY amp3 "10"> <!-- right/left amp  16 -->
        <!ENTITY amp4 "4"> <!-- forward/backward amp 20 -->
        <!ENTITY amp5 "24"> <!-- forward/backward amp 24 -->
        <!ENTITY period "0.25">
        <!ENTITY phase "1.570796327">
        ]>

<behavior name="SideLeft" type="ImprovedCPGBehavior">
    <constant name="amp1" value="12"/>
    <constant name="amp2" value="24"/>
    <constant name="amp3" value="10"/>
    <constant name="amp4" value="4"/>
    <constant name="amp5" value="24"/>
    <constant name="period" value="0.25"/>
    <constant name="phase" value="1.570796327"/>

    <patterns>
        &lleg2;: -amp3 period phase 0;
        &rleg2;: amp3 period phase 0;

        &lleg3;: amp1 period 0 28.44;   <!--- era 30   zero walk 16-->
        &rleg3;: -amp1 period 0 28.44;

        &lleg4;: -amp2 period 0 -46.33; <!-- zero wlk -29 -->
        &rleg4;: amp2 period 0 -46.33;

        &lleg5;: amp1 period 0 31;  <!-- zero walk 25 -->
        &rleg5;: -amp1 period 0 31;

        &lleg6;: amp3 period phase 0;
        &rleg6;: -amp3 period phase 0;

        &lleg1;: amp4 period phase 0;
        &rleg1;: amp4 period phase 0;

        &larm1;: 0 period 0 -90;
        &rarm1;: 0 period 0 -90;
    </patterns>

    <delta>period</delta>

</behavior>
