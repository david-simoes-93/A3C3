        &rleg6;: -&amp3; &period; 1.570796327 0;

        &lleg1;: -&amp4; &period; 1.570796327 -10;  <!-- era -10 -->
        &rleg1;: -&amp4; &period; 1.570796327 -10;

        &larm1;: 0 &period; 0 -90;
        &rarm1;: 0 &period; 0 -90;
